{
    "manifest_version": 3,
    "name": "Mark Movies as Seen",
    "description": "Mark movies as seen on the Hindi Movies 2025 Wikipedia page.",
    "version": "1.0",
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["*://*.wikipedia.org/wiki/List_of_Hindi_films_of_2025*"],
        "js": ["content.js"]
      }
    ]
  }
